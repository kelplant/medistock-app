<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <group
        android:translateX="54"
        android:translateY="54">

        <!-- Boîte de médicaments/stock en arrière-plan -->
        <group
            android:translateY="2">
            <!-- Rectangle de la boîte avec ombre -->
            <path
                android:fillColor="#E0F7FA"
                android:fillAlpha="0.4"
                android:pathData="M-18,-14 L18,-14 L18,16 L-18,16 Z"/>

            <!-- Détails de la boîte -->
            <path
                android:fillColor="#FFFFFF"
                android:fillAlpha="0.6"
                android:pathData="M-16,-12 L16,-12 L16,-8 L-16,-8 Z"/>
        </group>

        <!-- Croix médicale principale - Grande et moderne -->
        <group android:translateY="-2">
            <!-- Ombre de la croix pour profondeur -->
            <path
                android:fillColor="#004D40"
                android:fillAlpha="0.2"
                android:pathData="M-7,-24 L7,-24 L7,-7 L24,-7 L24,7 L7,7 L7,24 L-7,24 L-7,7 L-24,7 L-24,-7 L-7,-7 Z"
                android:translateX="2"
                android:translateY="2"/>

            <!-- Barre verticale de la croix -->
            <path
                android:fillColor="#FFFFFF"
                android:pathData="M-6,-24 L6,-24 L6,-6 L6,6 L6,24 L-6,24 L-6,6 L-6,-6 Z"/>

            <!-- Barre horizontale de la croix -->
            <path
                android:fillColor="#FFFFFF"
                android:pathData="M-24,-6 L-6,-6 L6,-6 L24,-6 L24,6 L6,6 L-6,6 L-24,6 Z"/>

            <!-- Centre brillant de la croix -->
            <path
                android:fillColor="#80DEEA"
                android:pathData="M-5,-5 L5,-5 L5,5 L-5,5 Z"/>

            <!-- Highlight supérieur pour effet 3D -->
            <path
                android:fillColor="#FFFFFF"
                android:fillAlpha="0.4"
                android:pathData="M-5,-22 L5,-22 L5,-18 L-5,-18 Z"/>
            <path
                android:fillColor="#FFFFFF"
                android:fillAlpha="0.4"
                android:pathData="M-22,-5 L-18,-5 L-18,5 L-22,5 Z"/>
        </group>

        <!-- Capsule de médicament - Design moderne -->
        <group
            android:translateX="16"
            android:translateY="-16"
            android:rotation="-45">

            <!-- Ombre de la capsule -->
            <path
                android:fillColor="#004D40"
                android:fillAlpha="0.2"
                android:pathData="M-9,-4 A4,4 0 0,1 -9,4 L9,4 A4,4 0 0,1 9,-4 Z"
                android:translateX="1"
                android:translateY="1"/>

            <!-- Moitié gauche - Rouge/Orange -->
            <path
                android:fillColor="#FF5252"
                android:pathData="M-9,-3.5 L0,-3.5 L0,3.5 L-9,3.5 A3.5,3.5 0 0,1 -9,-3.5 Z"/>

            <!-- Moitié droite - Blanc/Crème -->
            <path
                android:fillColor="#FFFFFF"
                android:pathData="M0,-3.5 L9,-3.5 A3.5,3.5 0 0,1 9,3.5 L0,3.5 Z"/>

            <!-- Contour brillant -->
            <path
                android:fillColor="#00000000"
                android:strokeColor="#FFFFFF"
                android:strokeWidth="1"
                android:pathData="M-9,0 A3.5,3.5 0 0,1 -9,-3.5 L9,-3.5 A3.5,3.5 0 0,1 9,3.5 L-9,3.5 A3.5,3.5 0 0,1 -9,0 Z"/>

            <!-- Ligne de séparation -->
            <path
                android:strokeColor="#FFB74D"
                android:strokeWidth="1.2"
                android:pathData="M0,-3.5 L0,3.5"/>

            <!-- Reflet sur la capsule -->
            <path
                android:fillColor="#FFFFFF"
                android:fillAlpha="0.5"
                android:pathData="M-7,-2 L-5,-2 L-5,0 L-7,0 Z"/>
        </group>

        <!-- Pilule ronde - coin inférieur gauche -->
        <group
            android:translateX="-16"
            android:translateY="16">
            <!-- Ombre -->
            <circle
                android:fillColor="#004D40"
                android:fillAlpha="0.2"
                cx="1"
                cy="1"
                r="5"/>
            <!-- Pilule -->
            <circle
                android:fillColor="#4DD0E1"
                cx="0"
                cy="0"
                r="5"/>
            <!-- Reflet -->
            <circle
                android:fillColor="#FFFFFF"
                android:fillAlpha="0.6"
                cx="-1.5"
                cy="-1.5"
                r="2"/>
        </group>

        <!-- Checkmark pour "stock validé" - coin inférieur droit -->
        <group
            android:translateX="17"
            android:translateY="17"
            android:scaleX="0.8"
            android:scaleY="0.8">
            <!-- Ombre du checkmark -->
            <path
                android:strokeColor="#004D40"
                android:strokeAlpha="0.2"
                android:strokeWidth="2.5"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:pathData="M-3,0 L-1,3 L4,-4"
                android:translateX="1"
                android:translateY="1"/>
            <!-- Checkmark -->
            <path
                android:strokeColor="#FFFFFF"
                android:strokeWidth="2.5"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:pathData="M-3,0 L-1,3 L4,-4"/>
        </group>

    </group>
</vector>
